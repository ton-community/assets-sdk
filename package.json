{
    "name": "@ton-community/assets-sdk",
    "version": "0.0.4",
    "main": "dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "assets-cli": "./dist/cli/cli.js"
    },
    "browser": {
        "@aws-sdk/client-s3": false,
        "@pinata/sdk": false,
        "stream": false,
        "boxen": false,
        "chalk": false
    },
    "scripts": {
        "build": "npm run clean && npm run compile && tsc",
        "clean": "rimraf ./dist",
        "compile": "ts-node ./src/jetton/contracts/compile.ts && ts-node ./src/nft/contracts/compile.ts",
        "prepare": "npm run compile",
        "dev": "tsc -w"
    },
    "files": [
        "dist/**/*"
    ],
    "dependencies": {
        "@aws-sdk/client-s3": "^3.423.0",
        "@orbs-network/ton-access": "^2.3.3",
        "@pinata/sdk": "^2.1.0",
        "axios": "^1.6.2",
        "dotenv": "^16.3.1",
        "inquirer": "^8.0.0",
        "zod": "^3.22.4"
    },
    "peerDependencies": {
        "@ton/core": ">=0.56.0",
        "@ton/crypto": ">=3.2.0",
        "@ton/ton": ">=13.11.0"
    },
    "devDependencies": {
        "@ton-community/func-js": "0.6.3",
        "@ton/core": "^0.56.0",
        "@ton/crypto": "^3.2.0",
        "@ton/ton": "^13.11.0",
        "@types/inquirer": "^9.0.7",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.3.2",
        "boxen": "^5.1.2",
        "chalk": "^4.1.2",
        "jest": "^29.7.0",
        "rimraf": "^5.0.5",
        "ton-crypto": "^3.2.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ton-community/assets-sdk.git"
    },
    "homepage": "https://github.com/ton-community/assets-sdk",
    "bugs": {
        "url": "https://github.com/ton-community/assets-sdk/issues"
    }
